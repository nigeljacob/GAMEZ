<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gamez"
        tools:targetApi="31">
        <activity
            android:name=".loadingActivity"
            android:exported="false"
            android:label="@string/title_activity_loading"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".IntroductionActivity"
            android:exported="false"
            android:label="@string/title_activity_introduction"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".Advanced"
            android:exported="false"
            android:label="@string/title_activity_advanced"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".GuessTheFlag"
            android:exported="false"
            android:label="@string/title_activity_guess_the_flag"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".GuessHints"
            android:exported="false"
            android:label="@string/title_activity_guess_hints"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".GuessTheCountry"
            android:exported="false"
            android:label="@string/title_activity_guess_the_country"
            android:theme="@style/Theme.Gamez" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Gamez">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>